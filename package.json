{
  "name": "tiffin-walla",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint ./src",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=0 'src/**/*.{js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
